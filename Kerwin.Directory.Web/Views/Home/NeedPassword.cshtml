@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<div class="row zy-main zy-pwd">
    <div class="col-lg-12 col-md-12">
        <h2 class="zy-pwd-tip">输入密码进行访问</h2>
    </div>
    <div class="col-lg-12 col-md-12">
        <form class="form-inline" method="post">
            <div class="form-group zy-pwd-item">
                <label for="password"> <i class="iconfont icon-mima"></i></label>
                <input id="password" class="form-control zy-pwd-input" type="password" name="accessPassword" value="" placeholder="访问密码" autofocus="autofocus" autocomplete="off" />
            </div>
            <div class="">
                <button type="submit" class="btn btn-default zy-pwd-btn"><i class="iconfont icon-fingerprint__"></i>访问</button>
            </div>
            <div class="form-group zy-pwd-item" style="color: cadetblue;">
                <span>@(ViewBag.Msg)</span>
            </div>
        </form>
    </div>
</div>